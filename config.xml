<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
    id          = "GAZ"
    versionCode = "21"
    version     = "1.0.0" >
    <!-- versionCode is optional and Android only -->

    <name>Farm Manager</name>

    <description>
      A simple productivity boost tool.
    </description>

    <author href="https://blackladder.co.za" email="daganread@gmail.com">
      Dagan Read
    </author>

    <platform name="android" >
        <preference name="android-minSdkVersion" value="19" />
        <preference name="android-targetSdkVersion" value="19" />
    </platform> 

    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="data:*" />
    <allow-intent href="*" />

    <!-- We'll include the contacts plugin as an example -->
    <plugin name="cordova-plugin-dialogs" spec="1.2.0" source="npm" />
    <plugin name="cordova-plugin-contacts" spec="2.0.1" source="npm" />
    <plugin name="cordova-plugin-whitelist" spec="1.2.1" source="npm" />
</widget>
